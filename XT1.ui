   <string notr="true">#centralwidget{
border-image: url(:/新前缀/img/bg.jpg);
}
QLabel{
color:#ffffff;
}
QPushButton{
padding:2px 2px 2px 2px;
background-color: rgba(255,255,255,100);
border-style:none;
border-radius: 5px;
color:#ffffff;
}

QPushButton:hover{
padding:2px 2px 2px 2px;
background-color: rgba(220, 220, 220,200);
border-radius: 5px;
}

QPushButton:pressed {
	padding:2px 2px 2px 2px;
    /* 改变背景色 */  
    background-color:rgba(180, 180, 180,100);  
    /* 改变边框风格 */  
    border-style:inset;  
    /* 使文字有一点移动 */
    padding-left:1px;
    padding-top:1px;
	border-radius: 5px;
}

QLineEdit{
background-color: rgba(255, 255, 255,120);
border: 1px solid #000000;
border-radius:3px;
color:#ffffff;
padding-left: 5px;
}
QLineEdit:hover
{
	border:1px solid rgba(31,156,220) ;
}

QComboBox{
background-color: rgba(255, 255, 255,120);
border: 1px solid #000000;
border-radius:3px;
color:#ffffff;
padding-left: 5px;
}
QComboBox:hover
{
    border:1px solid rgba(30,150,220);
}
/*点击combox的样式*/
QComboBox:on
{
	border-radius:3px;
	background-color: rgba(255, 255, 255,120);
    color:rgb(255,255,255);
    border:1px solid rgba(30,150,220);
}
/*下拉框的样式*/
QComboBox QAbstractItemView 
{
    outline: 0px solid gray;  /*取消选中虚线*/
    border:1px solid rgba(30,150,220);
    color: rgb(255,255,255);
	background-color: rgba(255, 255, 255,120);
    selection-background-color: rgb(90,90,90);   
}
 /*选中每一项高度*/
QComboBox QAbstractItemView::item
 { 
	height: 40px;  
 }
/*选中每一项的字体颜色和背景颜色*/
QComboBox QAbstractItemView::item:selected 
{
    color: rgb(31,163,246);
	background-color: rgb(90,90,90); 
}
 /*下拉箭头的边框*/
QComboBox::drop-down 
{
	border:none;
}
 /*下拉箭头样式*/
QComboBox::down-arrow 
{
    right:10px;/*控制箭头左右偏移*/
    width: 9px;  
    height: 9px;   
    image: url(:/新前缀/img/下箭头.png);
}
QTabWidget::pane{
border:none;
border-top:1px solid rgba(150,150,150,200);
}
#tab,#tab_2{
background-color: rgba(255, 255, 255,0);
}

QTabWidget::tab-bar {
     left: 5px;
}

QTabBar::tab {
